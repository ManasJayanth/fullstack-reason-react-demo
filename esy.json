    "@opam/dune": "3.5.0",
    "@opam/reason": "^3.8.1",
    "melange": "melange-re/melange",
    "ocaml": "4.14.0",
    "native-react": "*",
    "react-jsx-ppx": "*"
  },
  "devDependencies": { "@opam/ocaml-lsp-server": "*" },
  "resolutions": {
